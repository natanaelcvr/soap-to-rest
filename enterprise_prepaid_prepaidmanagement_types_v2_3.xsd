        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remtimeGrace" type="xsd:short" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Rem time Grace.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
