  <xsd:complexType name="OneTimeCCRechargeMethodInfoType">
    <xsd:annotation>
      <xsd:documentation>Credit card information details used for recharge.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="enterprise_prepaid_prepaidmanagement_xsd:CashRechargeMethodInfoType">
        <xsd:annotation>
          <xsd:documentation>Use this recharge method as the input when
						invoking the RPL API for performing one-time credit card recharge.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="ccLast4Digits" type="enterprise_prepaid_prepaidmanagement_xsd:CCLast4DigitsType">
            <xsd:annotation>
              <xsd:documentation>Last 4 digits of Credit card.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ccExpirationDate" type="xsd:date">
            <xsd:annotation>
              <xsd:documentation>Credit card expiration date.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditCardType" type="enterprise_prepaid_prepaidmanagement_xsd:CreditCardTypeType">
            <xsd:annotation>
              <xsd:documentation>Contains the credit card type, such as Visa
								(VS) or MasterCard (MC).</xsd:documentation>
            </xsd:annotation>
