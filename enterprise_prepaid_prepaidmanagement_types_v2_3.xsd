        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AnnivDate" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Anniversary date of customer</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SubscriberInfoType">
    <xsd:sequence>
      <xsd:element name="FirstMRC" type="xsd:byte">
        <xsd:annotation>
          <xsd:documentation>First MRC Indicator</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SubscriberStatus" type="xsd:byte">
        <xsd:annotation>
          <xsd:documentation>Subscriber Status from CM</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OfferDetailsInfo" type="enterprise_prepaid_prepaidmanagement_xsd:OfferDetailsInfoType" nillable="true" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Offer Information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrimResourceVal" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>MDN Value of subscriber</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ActivationDueAmt" type="xsd:double">
        <xsd:annotation>
          <xsd:documentation>For a new subscriber, it will depict the activation amount due, including ML discount if applicable</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SubCreationDate" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Subscriber Creation Date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SubStatusDate" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Subscriber Status change date from CM</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OfferDetailsInfoType">
    <xsd:sequence>
      <xsd:element name="DiscountAmt" type="xsd:double">
        <xsd:annotation>
          <xsd:documentation>Discount amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DiscountType" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Discount Type</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OfferDesc" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Offer Description</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OfferId" type="xsd:int">
        <xsd:annotation>
          <xsd:documentation>Offer ID</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OfferInstance" type="xsd:long">
        <xsd:annotation>
          <xsd:documentation>Offer Instance</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OfferLevel" type="xsd:byte">
        <xsd:annotation>
          <xsd:documentation>The level of the service can be C (subscriber-level service) or G (group-level service)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OfferName" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Name of BO/PO</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RCFrq" type="xsd:byte">
        <xsd:annotation>
          <xsd:documentation>Frequency of offer: Monthly/Daily</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RcAmt" type="xsd:double">
        <xsd:annotation>
          <xsd:documentation>RC Amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ServiceType" type="xsd:byte">
        <xsd:annotation>
          <xsd:documentation>Defines the type of the service (e.g., Price Plan, Regular SOC, Promotional PP, etc.).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
