        </xsd:annotation>
      </xsd:element>
      <xsd:element name="accumulatedRecharge" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Identifies the Business Entity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="accumulationStartDt" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Date when subscriber is created in RPL.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="annivDate" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Identifies the Business Entity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="chargeLftInd" type="xsd:byte" minOccurs="0">
