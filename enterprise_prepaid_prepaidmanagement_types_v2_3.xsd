<!-- 
Jira_Tracking_ID: DevJIRA-IPE-153
Build_Label: JDeveloperBuild
Build_Date: 2019-07-10T10:40:46.097-0500
--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" version="v2_2" targetNamespace="http://services.uscellular.com/schema/enterprise/prepaid/prepaidmanagement/v2_0/types" xmlns:enterprise_prepaid_prepaidmanagement_xsd="http://services.uscellular.com/schema/enterprise/prepaid/prepaidmanagement/v2_0/types" xmlns:enterprise_common_xsd="http://services.uscellular.com/schema/enterprise/common/v1_0/types" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:annotation>
    <xsd:documentation>ESB IDD#165 IDD#2012.</xsd:documentation>
  </xsd:annotation>
  <xsd:import namespace="https://github.com/natanaelcvr/soap-to-rest" schemaLocation="enterprise_common_types_v1_0.xsd"/>
  <!--Types-->
  <xsd:simpleType name="BucketIdType">
    <xsd:annotation>
      <xsd:documentation>The bucket ID.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int"/>
  </xsd:simpleType>
  <xsd:simpleType name="BucketTypeCodeType">
    <xsd:annotation>
      <xsd:documentation>Contains 2 character codes(e.g. UP for UPrepaid,
				PI for PiB and similarly for other policies).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="BusinessEntityType">
    <xsd:annotation>
      <xsd:documentation>The business entity of the payment channel number.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:pattern value="\d{0,9}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CCAuthRequestIDType">
    <xsd:annotation>
      <xsd:documentation>Credit Card Authentication Request ID.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="45"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CCAuthorizationCodeType">
    <xsd:annotation>
      <xsd:documentation>Authorization Code returned by Payment Gateway.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="45"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CCLast4DigitsType">
    <xsd:annotation>
      <xsd:documentation>Last 4 digits of Credit card.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="4"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CCMerchantIDType">
    <xsd:annotation>
      <xsd:documentation>A merchant ID allows a business to be identified
				by the banks and credit card institutions, in order to accept credit
				card payments.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="30"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CCMerchantRefIDType">
    <xsd:annotation>
      <xsd:documentation>Credit Card Merchant Reference ID.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="45"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ClientTransactionIdType">
    <xsd:annotation>
      <xsd:documentation>The client transaction ID (this can be populated
				when the client application generates their own transaction ID).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="15"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CoverDebitIndType">
    <xsd:annotation>
      <xsd:documentation>Indicates Indicates if the debt is covered.
				Possible values:
				Y: Debt is covered.
				N: Debt is not covered.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="Y|N"/>
      <xsd:minLength value="1"/>
      <xsd:maxLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CreditCardTypeType">
    <xsd:annotation>
      <xsd:documentation>Contains the credit card type, such as Visa (VS)
				or MasterCard (MC).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="2"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CurrencyCodeType">
    <xsd:annotation>
      <xsd:documentation>The recharge currency code.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="3"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ExemptionTypeType">
    <xsd:annotation>
      <xsd:documentation>The exemption type and possible values are None,
				Service, Customer</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="N|S|C"/>
      <xsd:minLength value="1"/>
      <xsd:maxLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="IdentifierType">
    <xsd:annotation>
      <xsd:documentation>The unique object identifier that was sent in the
				input.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:pattern value="\d{0,9}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="KeyType">
    <xsd:annotation>
      <xsd:documentation>The Key.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short"/>
  </xsd:simpleType>
  <xsd:simpleType name="NoOfRechargeDaysType">
    <xsd:annotation>
      <xsd:documentation>The recharge number of days.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int"/>
  </xsd:simpleType>
  <xsd:simpleType name="PaySourceIdType">
    <xsd:annotation>
      <xsd:documentation>The payment source ID.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="240"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PaySourceTypeType">
    <xsd:annotation>
      <xsd:documentation>The payment source type.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="Y|N"/>
      <xsd:minLength value="1"/>
      <xsd:maxLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PcnType">
    <xsd:annotation>
      <xsd:documentation>The payment channel number (a unique reference to
				the Online Charging payment account).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:pattern value="\d{0,9}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PinNumberType">
    <xsd:annotation>
      <xsd:documentation>Voucher serial number.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="PrepaidDateTimeType">
    <xsd:annotation>
      <xsd:documentation>UTC Time of authorization. Format:
				YYYY-MM-DDThh:mm:ssZ
				Example: 2005-08-11T22:47:57Z is
				equal to August 11, 2005, at 10:47:57 P.M.
				The T separates the date and the time.
				The Z indicates UTC. Milliseconds optional.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>
  <xsd:simpleType name="PrimaryResourceTypeType">
    <xsd:annotation>
      <xsd:documentation>The primary resource type.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="C|MDN"/>
      <xsd:minLength value="1"/>
      <xsd:maxLength value="3"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RcgStatusType">
    <xsd:annotation>
      <xsd:documentation>The payment source type.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="V|C"/>
      <xsd:minLength value="1"/>
      <xsd:maxLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ReasonCode">
    <xsd:annotation>
      <xsd:documentation>Indicates Indicates if the debt is covered.
				Possible values:
				Y: Debt is covered.
				N: Debt is not covered.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="50"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RechargeIdType">
    <xsd:annotation>
      <xsd:documentation>The recharge ID.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="RechargeTypeType">
    <xsd:annotation>
      <xsd:documentation>The recharge type.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="R|F"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ReferenceIdType">
    <xsd:annotation>
      <xsd:documentation>The payment channel ID. The channel (client
				application) application ID (this is stored as the main ID of the
				client application and can be populated with any value).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="45"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SelectCriteriaType">
    <xsd:annotation>
      <xsd:documentation>Voucher serial number.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="SerialNumberType">
    <xsd:annotation>
      <xsd:documentation>Voucher serial number.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="20"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StatusIndType">
    <xsd:annotation>
      <xsd:documentation>Status Indicator.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SubIdentifierType">
    <xsd:annotation>
      <xsd:documentation>The object sub-identifier that was sent in the
				input.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:pattern value="\d{0,9}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TaxAuthorityType">
    <xsd:annotation>
      <xsd:documentation>The tax authority (for example, 01).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="7"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TaxCodeType">
    <xsd:annotation>
      <xsd:documentation>The tax code.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="6"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TaxGeoCodeType">
    <xsd:annotation>
      <xsd:documentation>Geocode used for Tax calculation.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="10"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TaxItemSummaryType">
    <xsd:annotation>
      <xsd:documentation>Comma seperated list of tax items.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="256"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TaxTypeType">
    <xsd:annotation>
      <xsd:documentation>The tax type (for example, C99).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="6"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TimestampType">
    <xsd:annotation>
      <xsd:documentation>Long timestamp, in milliseonds.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:long"/>
  </xsd:simpleType>
  <xsd:simpleType name="UniqueValueType">
    <xsd:annotation>
      <xsd:documentation>Voucher serial number.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="VoucherIdType">
    <xsd:annotation>
      <xsd:documentation>Voucher ID.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <!--Structures-->
  <xsd:complexType name="AccumulationsAmountsInfoType">
    <xsd:annotation>
      <xsd:documentation>Contains details for Accumulations amounts
				Information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="instlAmt" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The installment amount.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="regBonusAmt" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The regular bonus amount.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="addBonusAmt" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The additional bonus amount.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="reduceAmt" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The reduce-by amount.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="debtAmt" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The debt amount.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="createChargeAmt" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The create charge amount.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="loanAmt" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The loan amount.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AgentsChannelInfoType">
    <xsd:annotation>
      <xsd:documentation>Concrete type for the Agents channel information.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="enterprise_prepaid_prepaidmanagement_xsd:BaseChannelInfoType">
        <!--Removed the below annotation and added above <xsd:annotation> <xsd:documentation>Concrete 
					type for the Agents channel information.</xsd:documentation> </xsd:annotation>-->
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AtmIdInfoType">
    <xsd:annotation>
      <xsd:documentation>Atm Id information.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="enterprise_prepaid_prepaidmanagement_xsd:BaseBucketIdentType">
        <!--Removed the below annotation and added above <xsd:annotation> <xsd:documentation>Atm 
					Id information.</xsd:documentation> </xsd:annotation>-->
        <xsd:sequence>
          <xsd:element name="atmBucketId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>The atm bucket ID.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AttributeType">
    <xsd:annotation>
      <xsd:documentation>Container holding name value pairs.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The name of the attribute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="value" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The value of the attribute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AttributeListType">
    <xsd:annotation>
      <xsd:documentation>Container holding name value pairs.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="attribute" type="enterprise_prepaid_prepaidmanagement_xsd:AttributeType" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>A set of attributes for journaling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BaseBucketIdentType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Base Bucket identifaction information.</xsd:documentation>
    </xsd:annotation>
  </xsd:complexType>
  <xsd:complexType name="BaseCancelRechargeMethodType">
    <xsd:annotation>
      <xsd:documentation>Use this recharge method as the input when
				invoking the RPL API to cancel recharge.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="enterprise_prepaid_prepaidmanagement_xsd:BaseRechargeMethodType">
        <!--Removed the below annotation and added above <xsd:annotation> <xsd:documentation>Use 
					this recharge method as the input when invoking the RPL API to cancel recharge.</xsd:documentation> 
					</xsd:annotation>-->
        <xsd:sequence>
          <xsd:element name="rechargeId" type="enterprise_prepaid_prepaidmanagement_xsd:RechargeIdType">
            <xsd:annotation>
              <xsd:documentation>The recharge ID.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="rechargeType" type="enterprise_prepaid_prepaidmanagement_xsd:RechargeTypeType">
            <xsd:annotation>
              <xsd:documentation>The recharge type.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="BaseChannelInfoType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Contains details for Agents, IVR channel or Credit Processor
				information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="referenceId" type="enterprise_prepaid_prepaidmanagement_xsd:ReferenceIdType">
        <xsd:annotation>
          <xsd:documentation>The payment channel ID. The channel (client
						application) application ID (this is stored as the main ID of the
						client application and can be populated with any value).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="requestDateTime" type="enterprise_prepaid_prepaidmanagement_xsd:PrepaidDateTimeType">
        <xsd:annotation>
          <xsd:documentation>The payment channel request date. This is the
						date and time that the channel requests the activity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="clientTransactionId" type="enterprise_prepaid_prepaidmanagement_xsd:ClientTransactionIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The client transaction ID (this can be populated
						when the client application generates their own transaction ID).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BaseRechargeMethodType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Base recharge method information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="paySourceId" type="enterprise_prepaid_prepaidmanagement_xsd:PaySourceIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The payment source ID.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="paySourceType" type="enterprise_prepaid_prepaidmanagement_xsd:PaySourceTypeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The payment source type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BaseRegularRechargeMethodType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Use this recharge method as the input when
				invoking the RPL API for performing cash recharge.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="enterprise_prepaid_prepaidmanagement_xsd:BaseRechargeMethodType">
        <!--Removed the below annotation and added above <xsd:annotation> <xsd:documentation>Use 
					this recharge method as the input when invoking the RPL API for performing 
					cash recharge.</xsd:documentation> </xsd:annotation>-->
        <xsd:sequence>
          <xsd:element name="amount" type="xsd:double">
            <xsd:annotation>
              <xsd:documentation>The recharge amount excluding tax (amount to
								be added to the balance).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="currency" type="enterprise_prepaid_prepaidmanagement_xsd:CurrencyCodeType">
            <xsd:annotation>
              <xsd:documentation>The recharge currency code (as defined in ISO
								4217).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="BaseVoucherRechargeMethodType">
    <xsd:annotation>
      <xsd:documentation>Use this recharge method as the input when
				invoking the RPL API for performing voucher recharge.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="enterprise_prepaid_prepaidmanagement_xsd:BaseRechargeMethodType">
        <!--Removed the below annotation and added above <xsd:annotation> <xsd:documentation>Use 
					this recharge method as the input when invoking the RPL API for performing 
					voucher recharge.</xsd:documentation> </xsd:annotation>-->
        <xsd:sequence>
          <xsd:element name="serialNumber" type="enterprise_prepaid_prepaidmanagement_xsd:SerialNumberType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Voucher serial number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="coverDebitInd" type="enterprise_prepaid_prepaidmanagement_xsd:CoverDebitIndType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if the debt is covered.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="pinNumber" type="enterprise_prepaid_prepaidmanagement_xsd:PinNumberType">
            <xsd:annotation>
              <xsd:documentation>Voucher pin number.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="selectCriteria" type="enterprise_prepaid_prepaidmanagement_xsd:SelectCriteriaType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Voucher select criteria.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="uniqueValue" type="enterprise_prepaid_prepaidmanagement_xsd:UniqueValueType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Voucher unique value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="BucketBalanceInformationType">
    <xsd:annotation>
      <xsd:documentation>Bucket and Balance details fetched from Amdocs
				Balance Manager system.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="bucketHeader" type="enterprise_prepaid_prepaidmanagement_xsd:BucketHeaderType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Bucket header information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="olcQueryBalanceInfo" type="enterprise_prepaid_prepaidmanagement_xsd:OlcQueryBalanceInfoType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Olc query balance information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rpl9BucketAdditionalInfo" type="enterprise_prepaid_prepaidmanagement_xsd:RPL9BucketAdditionalInfoType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Bucket addition information. Subscriber Status,
						Rechrge amount, Anniversary date and Product type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BucketGeneralInfoType">
    <xsd:annotation>
      <xsd:documentation>Bucket general information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="policyCode" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The Policy code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="currency" type="enterprise_prepaid_prepaidmanagement_xsd:CurrencyCodeType">
        <xsd:annotation>
          <xsd:documentation>The currency code (as defined in ISO 4217).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="billCycle" type="xsd:short">
