	<xsd:complexType name="CatalogAttachmentType">
		<xsd:annotation>
			<xsd:documentation>Defines catalogAttachment structure.</xsd:documentation>
		</xsd:annotation>
    	<xsd:sequence>
            <xsd:element name="attachmentType" type="enterprise_common_xsd:AttachmentTypeType" minOccurs="1">
            	<xsd:annotation>
                	<xsd:documentation>
                		Type of the attachment. 
                		For e.g. 'ECOMNAME' (E-commerce Name), 
                				 'ECOMDESC' (E-commerce Description), 
                				 'LVPIC' (List view picture).
                	</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
        	<xsd:element name="attachmentText" type="enterprise_common_xsd:AttachmentTextType" minOccurs="1">
            	<xsd:annotation>
